{
  "name": "healthz-prober",
  "version": "1.0.0",
  "description": "probes healthz endpoints and reports to slack",
  "main": "index.ts",
  "scripts": {
    "start": "node --watch dist/index.js",
    "start:watch": "node --watch dist/index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "(start npm run build:watch || exec npm run build:watch) & npm run start & wait",
    "clean": "rm -rf dist || rmdir /s/q dist || rmdir -r -Force dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/exxocism/node-healthz-prober.git"
  },
  "keywords": [
    "node",
    "healthz",
    "prober",
    "slack",
    "report"
  ],
  "author": "exxocism (Charlie)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/exxocism/node-healthz-prober/issues"
  },
  "homepage": "https://github.com/exxocism/node-healthz-prober#readme",
  "devDependencies": {
    "postject": "^1.0.0-alpha.6",
    "typescript": "^5.3.2"
  }
}
